

#1.相亲客户表
#DROP TABLE `BLIND_DATE_CUSTOMER`;
CREATE TABLE `BLIND_DATE_CUSTOMER` (
 `CUSTOMER_ID` VARCHAR(64)  NOT NULL PRIMARY KEY COMMENT '主键ID',
 `NAME` VARCHAR(200)  NOT NULL  COMMENT '姓名',
 `GENDER` VARCHAR(2)  NOT NULL COMMENT '性别',
 `PHONE_NUMBER` VARCHAR(20)  NULL COMMENT '手机号码',
 `CONTACT_INFORMATION` VARCHAR(100)  NULL COMMENT '其他联系方式',
 `BIRTHDATE` DATE  NOT NULL COMMENT '出生日期',
 `AGE` VARCHAR(20)   NULL COMMENT '年龄',
 `ZODIAC` VARCHAR(10)   NULL COMMENT '生肖（属相）',
 `CONSTELLATION` VARCHAR(20)   NULL COMMENT '星座',
 `ROOM` VARCHAR(4)   NULL COMMENT '是否有房',
 `VEHICLE` VARCHAR(4)   NULL COMMENT '是否有车',
 `ASSETSEXPLAIN` VARCHAR(2000)   NULL COMMENT '资产说明',
 `PHOTO` VARCHAR(64)   NULL COMMENT '照片路径',
 `CREATE_USER_ID` VARCHAR(64)  NOT NULL COMMENT '创建人ID',
 `CREATE_ORG_ID` VARCHAR(64)  NOT NULL COMMENT '创建人机构ID',
 `CREATE_DATE` DATE NOT NULL COMMENT '创建时间',
 `UPDATE_USER_ID` VARCHAR(64)   NULL COMMENT '修改人ID',
 `UPDATE_ORG_ID` VARCHAR(64)   NULL COMMENT '修改人机构ID',
 `UPDATE_DATE` DATE  NULL COMMENT '修改时间',
 `IS_DELETE` VARCHAR(2) NOT NULL COMMENT '删除标识（0删除，1未删除）'
) COMMENT = '相亲客户表' ENGINE = INNODB CHARACTER SET = UTF8 COLLATE = UTF8_GENERAL_CI ROW_FORMAT = COMPACT;

